{
  "name": "reader-origin",
  "version": "1.0.0",
  "description": "A simple article reader using React.",
  "private": true,
  "entry point": "src/server/index.js",
  "scripts": {
    "init-database": "READER_ENV=dev node ./spec/insertData.js",
    "dbmigrate-dev": "./node_modules/.bin/db-migrate up --env dev",
    "spec-dev": "READER_ENV=dev ./node_modules/.bin/jasmine-node --verbose --captureExceptions ./spec",
    "test": " rm /var/tmp/reader.test.sqlite && ./node_modules/.bin/db-migrate up --env test && READER_ENV=test ./node_modules/.bin/jasmine-node --verbose --captureExceptions ./spec",
    "start-client": "webpack-dashboard -- webpack-dev-server --hot --host 0.0.0.0 --port 5000",
    "start-server": "DEBUG=express:* READER_ENV=dev node src/server/index.js",
    "build": "rm -r ./src/client/dist && NODE_ENV=production webpack -p"
  },
  "author": "Shuang Qiu",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "db-migrate": "^0.9.26",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.2",
    "mysql": "^2.15.0",
    "mz": "^2.7.0",
    "node-dbi": "^0.7.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-loadable": "^5.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "spdy": "^3.4.7"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "he": "^1.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-node": "^1.14.5",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "react-transition-group": "^2.3.1",
    "request": "^2.87.0",
    "sass-loader": "^7.0.1",
    "sqlite3": "^4.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.14.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.1.5",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.4"
  }
}
